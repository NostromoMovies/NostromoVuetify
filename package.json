{
  "name": "nostromovite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run setup-backend && run-p type-check \"build-only {@}\" --",
    "dev": "concurrently \"npm run start-backend\" \"vite\"",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "setup-backend": "node -e \"const fs = require('fs'); const { execSync } = require('child_process'); if (!fs.existsSync('backend')) { execSync('git clone https://github.com/NostromoMovies/NostromoServer.git backend', { stdio: 'inherit' }); execSync('cd backend/Nostromo.Server && dotnet restore', { stdio: 'inherit' }); }\"",
    "start-backend": "dotnet run --project backend/Nostromo.TrayService/Nostromo.TrayService.csproj"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@mdi/font": "^7.4.47",
    "axios": "^1.7.9",
    "axois": "^0.0.1-security",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.6"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/axios": "^0.9.36",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "vite": "^6.0.7",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10",
    "concurrently": "^8.2.0"
  }
}
